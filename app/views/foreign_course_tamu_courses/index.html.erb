<p id="notice"><%= notice %></p>

<h1>Foreign Course Tamu Courses</h1>

<table>
  <thead>
    <tr>
      <th>Foreign course</th>
      <th>Tamu course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foreign_course_tamu_courses.each do |foreign_course_tamu_course| %>
      <tr>
        <td><%= foreign_course_tamu_course.foreign_course_id %></td>
        <td><%= foreign_course_tamu_course.tamu_course_id %></td>
        <td><%= link_to 'Show', foreign_course_tamu_course %></td>
        <td><%= link_to 'Edit', edit_foreign_course_tamu_course_path(foreign_course_tamu_course) %></td>
        <td><%= link_to 'Destroy', foreign_course_tamu_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foreign Course Tamu Course', new_foreign_course_tamu_course_path %>
